    "@eslint/compat": "^1.2.6",
    "@eslint/js": "^9.20.0",
    "@playwright/test": "^1.48.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@storybook/addon-docs": "^9.0.4",
    "@storybook/addon-links": "^9.0.4",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.6",
