    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.11.3",
    "firebase": "^11.3.1",
    "globals": "^15.15.0",
    "html-webpack-plugin": "^5.5.1",
    "husky": "^9.1.7",
    "jest": "^29.6.4",
