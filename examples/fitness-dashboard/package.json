{
  "name": "@ssa-ui-kit/fitness-dashboard",
  "version": "0.0.0",
  "main": "src/index.tsx",
  "private": true,
  "author": "SSA Group",
  "license": "MIT",
  "description": "Fitness Dashboard based on SSA UI Kit",
  "keywords": [
    "react",
    "SSA UI Kit",
    "Dashboard"
  ],
  "dependencies": {
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@floating-ui/react": "0.27.4",
    "@nivo/core": "^0.88.0",
    "@nivo/line": "^0.88.0",
    "@nivo/pie": "^0.88.0",
    "@nivo/scales": "^0.88.0",
    "@ssa-ui-kit/core": "workspace:^",
    "@ssa-ui-kit/hooks": "workspace:^",
    "@ssa-ui-kit/widgets": "workspace:^",
    "dotenv": "^16.0.3",
    "loose-envify": "^1.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "./jest-setup.ts"
    ],
    "transform": {
      "^.+\\.(j|t)sx?$": "ts-jest"
    },
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/apis/sources/mock/"
    ],
    "moduleNameMapper": {
      "^@components(.*)$": "<rootDir>/src/components$1",
      "^@apis(.*)$": "<rootDir>/src/apis$1",
      "^@hooks(.*)$": "<rootDir>/src/hooks$1"
    }
  },
  "scripts": {
    "test": "jest -i --no-cache",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve",
    "create": "node ./tasks/create.js",
    "analyze": "webpack --profile --json > stats.json; webpack-bundle-analyzer ./stats.json",
    "analyze:prod": "webpack --mode=production --node-env=production --profile --json > stats.json; webpack-bundle-analyzer ./stats.json",
    "ts-show-config": "tsc --showConfig -p ./tsconfig.json",
    "ts-check": "tsc -p ./tsconfig.json"
  },
  "browserslist": [
    ">0.1%",
    "not dead",
    "not op_mini all"
  ],
  "devDependencies": {
    "@emotion/jest": "^11.13.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "html-webpack-plugin": "^5.5.1",
    "terser-webpack-plugin":"^5.3.11",
    "webpack": "^5.98.0"
  }
}
