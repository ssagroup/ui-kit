    "ts-show-config-build": "tsc --showConfig -p ./tsconfig.build.json",
    "ts-check": "tsc -p ./tsconfig.json",
    "ts-check-build": "tsc -p ./tsconfig.build.json"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.5",
    "extend-expect": "^1.0.2",
    "resolve-tspaths": "^0.8.16",
