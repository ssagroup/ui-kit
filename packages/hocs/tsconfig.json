{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "jsxImportSource": "@emotion/react",
    "noEmit": false,
    "outDir": "./dist",
    "tsBuildInfoFile": "./tsbuildcache",
    "typeRoots": [
      "./node_modules/@types",
      "../../node_modules/@types"
    ],
    "paths": {
      "@hocs/*": ["./src/hocs/*"]
    }
  },
  "include": ["src/**/*", "./jest-setup.ts", "customTest.tsx"],
  "exclude": ["dist/**/*"]
}
