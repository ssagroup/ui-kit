{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "jsxImportSource": "@emotion/react",
    "emitDeclarationOnly": false,
    "typeRoots": [
      "./src/types",
      "./node_modules/@types",
      "../../node_modules/@types"
    ],
    "paths": {
      "@components/*": ["./src/components/*"]
    }
  },
  "include": ["src/**/*", "./jest-setup.ts", "./customTest.tsx", ".storybook/*.tsx"],
  "exclude": ["dist/**/*"]
}
