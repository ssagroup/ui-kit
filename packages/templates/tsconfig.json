{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "jsxImportSource": "@emotion/react",
    "emitDeclarationOnly": false,
    "typeRoots": [
      "./src/types",
      "./node_modules/@types",
      "../../node_modules/@types"
    ],
    "paths": {
      "@icons": ["./src/icons"],
      "@contexts": ["./src/contexts"],
      "@": ["./src/projects"],
      "@/*": ["./src/projects/*"],
      "@trading": ["./src/projects/trading"],
      "@trading/*": ["./src/projects/trading/*"],
      "@fitness": ["./src/projects/fitness"],
      "@fitness/*": ["./src/projects/fitness/*"],
      "@peopleops": ["./src/projects/peopleops"],
      "@peopleops/*": ["./src/projects/peopleops/*"]
    }
  },
  "include": [
    "src/**/*",
    "./jest-setup.ts",
    "./customTest.tsx",
    "./src/types/svg.d.ts"
  ],
  "exclude": ["dist/**/*"]
}
