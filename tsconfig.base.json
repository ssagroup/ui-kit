{
  "compilerOptions": {
    "incremental": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "allowJs": true,

    "strict": true,
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "target": "es6",
    "module": "esnext",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "resolveJsonModule": true,
    "lib": ["dom", "dom.iterable", "esnext"]
  }
}
